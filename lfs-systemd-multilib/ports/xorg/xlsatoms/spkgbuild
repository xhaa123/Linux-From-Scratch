# description   : List interned atoms defined on server
# depends       : libxcb
    
name=xlsatoms
version=1.1.3
release=1

source="https://www.x.org/pub/individual/app/$name-$version.tar.bz2"

build() {

	cd $name-$version

	./configure --prefix=/usr           \
	            --sysconfdir=/etc       \
	            --localstatedir=/var    \
	            --disable-static

	make
    
	make DESTDIR=$PKG install
}
